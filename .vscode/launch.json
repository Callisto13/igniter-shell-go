    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch tf2",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "launch",
                "--emulate-tty",
                "--config-file",
                "example/tf2.json",
                "--variable",
                "message==yeb=",
                "--",
                "docker",
                "run",
                "-ti",
                "docker.gameye.com/tf2"
            ]
        },
        {
            "name": "Launch controlpoint",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "launch",
                "--emulate-tty",
                "--config-file",
                "example/controlpoint.yaml",
                "--",
                "docker",
                "run",
                "-ti",
                "docker.gameye.com/tf2"
            ]
        },
        {
            "name": "Launch on-off",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "launch",
                "--config-file",
                "example/on-off.json",
                "--",
                "bash"
            ]
        }
    ]
}